.unlogin-body{
  background-color: #fff;
  width:100vw;
  height: 100vh;
}

.unlogin{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding-top: 40%;
}

.unlogin .unlogin-image{
  width: 150rpx;
  height: 150rpx;
}

.unlogin .unlogin-btn{
  margin-top: 50rpx;
  width:80%;
  background-color: rgb(86, 152, 238);
  color:#fff;
  height:100rpx;
}

.info{
  background-color: rgb(127, 189, 240);
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  padding: 100rpx 0;
}

.info .avatarUrl{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}

.cell-body{
   position: relative;
}

.cell-List{
  display: flex;
  flex-direction:column;
  align-items: center;
  justify-content: center;
  position: absolute;
  width: 100%;
  top: -20rpx;
}

.cell{
    display: flex;
    justify-content: space-between;
    width: 85%;
    background-color: #fff;
    padding: 30rpx 20rpx;
    border-radius: 20rpx;
    align-items: center;
    margin-bottom: 20rpx;
}

.cell-left{
   display: flex;
   align-items: center;
}

.cell-left .cell-left-image{
  width: 50rpx;
  height: 50rpx;
}

.cell-left .cell-left-text{
   margin-left: 20rpx;
}

.cell-right .cell-right-image{
  width: 50rpx;
  height: 50rpx;
}